{
    "compilerOptions": {
        "outDir": "./app/dist/js", //pasta de saída
        "target": "ES6",
        "noEmitOnError": true,
        "noImplicitAny": true,
        "removeComments": true, //remover comentários no JS compilado
        "strictNullChecks": true //retirando para que qualquer tipo possa receber null
    },
    "include": ["app/**/*"]
}